<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Monthly_Reserve__c</fullName>
    <description>Reserve created on a Monthly Basis</description>
    <externalId>false</externalId>
    <formula>(( Amount__c  -  Bank_Extras__c  -  Yearly_Reserve__c ) * 15)/100</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>15% Reserve created on a Monthly basis</inlineHelpText>
    <label>Monthly Reserve</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
