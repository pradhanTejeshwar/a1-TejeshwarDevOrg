<apex:page controller="TrialController">
    <apex:form >
        <apex:commandButton value="Search" action="{! foobar }"/>
    </apex:form>
    
    <apex:pageBlock >
    	<apex:pageBlockTable value="{!accountsList}" var="account">
        	<apex:column value="{!account.Name}"/>
            <apex:column value="{!account.Rating}"/>
            <apex:column value="{!account.Industry}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>